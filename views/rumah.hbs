                    <div class="card">
                        <div class="header">
                            <h2>
                                Information
                                <small>Have a nice day.</small>
                            </h2>
                        </div>
                        <div class="body table-responsive">
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>User ID</th>
                                        <th>Email</th>
                                        <th>Username</th>
                                        <th>Apikey</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>Kocak{{userid}}Z</td>
                                        <td>{{email}}</td>
                                        <td>{{nama}}</td>
                                        <td><p id="apikey">{{apikey}}</p></td>
                                        <td><a  href="#" onclick="CopyToClipboard('apikey');return false;" class="btn btn-sm btn-outline-secondary">COPY</a></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
